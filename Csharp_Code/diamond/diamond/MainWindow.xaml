<Window x:Class="diamond.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:win="clr-namespace:System.Windows;assembly=PresentationFramework"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:ViewModel="clr-namespace:diamond.ViewModel"
        xmlns:VIew="clr-namespace:diamond.VIew"
        xmlns:local="clr-namespace:diamond.VIew"
        mc:Ignorable="d"
        x:Name="thevoid"
        Title="MainWindow" Height="600" Width="850">
    <Window.DataContext>
        <ViewModel:MainWindowViewModel/>
    </Window.DataContext>
    <Grid>
        <ContentControl Content="{Binding ContentView}"/>
        <!--<Frame x:Name="frame" Source="/VIew/mainpage.xaml" NavigationUIVisibility="Hidden"/>-->
    </Grid>
</Window>
